{
  "dependencies": {
    "chalk": "^5.6.2"
  },
  "devDependencies": {
    "@alienfast/logger": "^12.1.2",
    "clean-package": "^2.2.0",
    "tsdown": "^0.15.6",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": "./src/index.ts",
    "./initializeNodeLogger": "./src/initializeNodeLogger.ts",
    "./NodeLogWriter": "./src/NodeLogWriter.ts",
